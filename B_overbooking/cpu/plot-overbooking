#!/bin/gnuplot

#set term png size 1500,900
#set output 'result-overbooking.png'
set terminal pdf size 8,4.8
set output 'result-overbooking.pdf'

#x axis formatting
set xlabel 'Time'
#set xdata time
#set timefmt "%s"
#set format x "%H:%M:%S"

#y axis formatting
set ylabel 'CPU Steal [%]'

# styles
set style line 1 lt 1 lw 2 lc rgb "#3a3a3a"
set style line 2 lt 2 lw 1 lc rgb "#aaaaaa"

set autoscale

plot "result-overbooking.csv" using :1:2 with errorbars ls 2 title 'Standard Deviation', "" using :1 with lines ls 1 title 'VMs Total'